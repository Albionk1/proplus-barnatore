<!DOCTYPE html>
<!--
Author: Thana Dev 2023
Product Name: Services
-->
<html lang="en">
<!--begin::Head-->

<head>
    <!--Begin Meta links-->
    <%- include ('includes/meta-links.ejs') %>
        <!--End Meta links-->
        <link href="/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />

</head>
<!--end::Head-->
<!--begin::Body-->

<body onload="startClock()" id="kt_app_body" data-kt-app-layout="dark-sidebar" data-kt-app-header-fixed="true"
    data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true"
    data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true"
    data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
    <!--begin::Theme mode setup on page load-->

    <!--end::Theme mode setup on page load-->
    <!--begin::App-->
    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
            <!--begin::Header-->
            <%- include('./includes/header.ejs') %>
                <!--end::Header-->
                <!--begin::Wrapper-->
                <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
                    <!--begin::Sidebar-->
                    <%- include('./includes/sidebar.ejs') %>
                        <!--end::Sidebar-->
                        <!--begin::Content-->
                        <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                            <!--begin::Post-->
                            <div class="post d-flex flex-column-fluid" id="kt_post">
                                <!--begin::Container-->
                                <div id="kt_content_container" class="container-fluid">
                                    <!--begin::Card body-->
                                    <form id="form-comp-auth" class="mt-7">
                                        <!--begin::Tab Content-->
                                        <div class="tab-content">
                                            <!--begin::Tab panel-->
                                            <div class=" p-0 tab-pane fade show active">
                                                <!--begin::Content wrapper-->
                                                <div class="d-flex flex-column flex-column-fluid card">
                                                    <!--begin::Card body-->
                                                    <div class="card-body border-top pt-5">
                                                        <!--begin::Input group-->
                                                        <!--begin::Row-->
                                                        <div class="row ">
                                                            <!--begin::Col-->
                                                            <div class="col-lg-3 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-3 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Startimi</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input class="form-control"
                                                                        placeholder="Zgjedh datën"
                                                                        id="kt_datepicker_1" />
                                                                    <p id="start_time-error" class="text-danger show-errors mb-0"></p>

                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-lg-3 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-3 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Finish</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input class="form-control"
                                                                        placeholder="Zgjedh datën"
                                                                        id="kt_datepicker_2" />
                                                                    <p id="end_time-error" class="text-danger show-errors mb-0"></p>

                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <div class="col-lg-3 col-sm-6">
                                                                <div class="fv-row mb-3">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3"
                                                                        style="white-space: nowrap;">Koment
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <textarea class="form-control form-control-solid"
                                                                        name="" rows="2" id="comment"
                                                                        placeholder="Shëno koment"></textarea>
                                                                    <p id="comment-error" class="text-danger show-errors mb-0"></p>

                                                                    <!--end::Input group-->
                                                                </div>
                                                            </div>
                                                            <!--begin::Col-->
                                                            <div class="col-lg-3 col-sm-6 mt-sm-11 mt-3">
                                                                <!--begin::Actions-->
                                                                <div class=" d-flex justify-content-end">
                                                                    <button type="submit" class="btn btn-primary w-100"
                                                                        id="" style="white-space: nowrap;">Ruaj</button>
                                                                </div>
                                                                <!--end::Actions-->
                                                            </div>
                                                            <!--end::Col-->
                                                        </div>
                                                        <!--end::Row-->
                                                    </div>
                                                </div>
                                                <!--end::Toolbar container-->
                                            </div>
                                            <!--end::Toolbar-->
                                        </div>
                                        <!--end::Content wrapper-->
                                    </form>
                                    <!--begin::Toolbar-->
                                    <div class="card-toolbar mt-5 w-100 ">
                                        <!--begin::Tab nav-->
                                        <ul class="nav nav-tabs nav-line-tabs nav-stretch fs-6 border-0 fw-bold entry-sistem"
                                            role="tablist">
                                            <li class="nav-item" role="presentation">
                                                <a id="company_tab"
                                                    class="nav-link justify-content-center text-active-gray-800 active"
                                                    data-bs-toggle="tab" role="tab" href="#company">Kompania
                                                </a>
                                            </li>
                                            
                                           
                                        </ul>
                                        <!--end::Tab nav-->

                                    </div>
                                    <!--end::Toolbar-->
                                    <!--begin::Card body-->
                                    <div class="card-body">
                                        <!--begin::Tab Content-->
                                        <div class="tab-content">
                                            <!--begin::Tab panel-->
                                            <div id="company" class="card-body p-0 tab-pane fade show active"
                                                role="tabpanel" aria-labelledby="company_tab">
                                                <!--begin::Content wrapper-->
                                                <div class="d-flex flex-column flex-column-fluid">
                                                    <!--begin::Content-->
                                                    <div id="kt_app_content" class="app-content flex-column-fluid">
                                                        <!--begin::Content container-->
                                                        <div id="kt_app_content_container">
                                                            <!--begin:::Tab content-->
                                                            <div class="tab-content" id="myTabContent">
                                                                <!--begin::Content-->
                                                                <div id="kt_app_content" class=" flex-column-fluid">
                                                                    <!--begin::Content container-->
                                                                    <div id="kt_app_content_container">
                                                                        <!--begin::Products-->
                                                                        <div class="card card-flush mt-3">
                                                                            <!--begin::Card header-->
                                                                            <div
                                                                                class="card-header align-items-center py-5 gap-2 gap-md-5">
                                                                                <!--begin::Card title-->
                                                                                <div class="card-title">
                                                                                    <!--begin::Search-->
                                                                                    <div
                                                                                        class="d-flex align-items-center position-relative my-1">
                                                                                        <i
                                                                                            class="ki-duotone ki-magnifier fs-3 position-absolute ms-4">
                                                                                            <span class="path1"></span>
                                                                                            <span class="path2"></span>
                                                                                        </i>
                                                                                        <input type="text"
                                                                                            data-kt-ecommerce-order-filter="search"
                                                                                            class="form-control form-control-solid w-250px ps-12"
                                                                                            placeholder="Kërko..." id="search-company" />
                                                                                    </div>
                                                                                    <!--end::Search-->
                                                                                </div>
                                                                                <!--end::Card title-->
                                                                            </div>
                                                                            <!--end::Card header-->
                                                                            <!--begin::Card body-->
                                                                            <div class="card-body pt-0">
                                                                                <div class="table-responsive">
                                                                                    <!--begin::Table-->
                                                                                    <table
                                                                                        class="table align-middle table-row-dashed fs-6 gy-5"
                                                                                        id="company-auth">
                                                                                        <thead>
                                                                                            <tr
                                                                                                class="text-start text-gray-800 fw-bold fs-6 text-uppercase gs-0">
                                                                                                <th class="w-10px">
                                                                                                    Nr
                                                                                                </th>
                                                                                                <th class="w-100px">
                                                                                                    Abonimi Code
                                                                                                </th>
                                                                                                <th class="w-50px">
                                                                                                    Start</th>
                                                                                                <th class="w-50px">
                                                                                                    Finish</th>
                                                                                                <th class="w-50px">
                                                                                                    Status</th>
                                                                                                <th class="w-100px">
                                                                                                    Koment</th>
                                                                                                <th
                                                                                                    class="text-center w-10px">
                                                                                                    Opsionet</th>
                                                                                            </tr>
                                                                                        </thead>
                                                                                        <tbody class="fw-semibold text-gray-700">
                                                                                         
                                                                                        </tbody>
                                                                                    </table>
                                                                                    <!--end::Table-->
                                                                                </div>
                                                                            </div>
                                                                            <!--end::Card body-->
                                                                        </div>
                                                                        <!--end::Products-->
                                                                    </div>
                                                                    <!--end::Content container-->
                                                                </div>
                                                                <!--end::Content-->
                                                            </div>
                                                            <!--end:::Tab content-->
                                                        </div>
                                                        <!--end::Content container-->
                                                    </div>
                                                    <!--end::Content-->
                                                </div>
                                                <!--end::Content wrapper-->
                                            </div>
                                            <!--end::Tab panel-->
                                            <!--begin::Tab panel-->
                                            
                                            <!--end::Tab panel-->
                                        </div>
                                        <!--end::Tab Content-->
                                    </div>
                                    <!--end::Card body-->
                                </div>
                                <!--end::Container-->
                            </div>
                            <!--end::Post-->
                        </div>
                        <!--end::Content-->

                        <%- include('./includes/footer.ejs') %>
                </div>
                <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->
    <!--begin::Javascript-->
    <%- include('./includes/script.ejs') %>

        <!--end::Javascript-->
        <script src="/assets/plugins/custom/datatables/datatables.bundle.js"></script>
        <!--begin::Custom Javascript(used for this page only)-->
        <script src="/assets/js/custom/apps/ecommerce/customers/listing/search/listing-5.js"></script>

        <script src="/assets/js/custom/apps/ecommerce/customers/listing/search/listing-out-cash.js"></script>
        <script src="/assets/js/utils.js"></script>
        <script src="/assets/js/thanadev/authorization.js"></script>
        <script>
            $("#kt_datepicker_2").flatpickr();
        </script>

        <!--end::Javascript-->
</body>
<!--end::Body-->

</html>