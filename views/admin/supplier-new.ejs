<!DOCTYPE html>
<!--
Author: Thana Dev 2023
Product Name: Services
-->
<html lang="en">
<!--begin::Head-->

<head>
    <!--Begin Meta links-->
    <%- include ('includes/meta-links.ejs') %>
        <!--End Meta links-->
        <link href="/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />

</head>
<!--end::Head-->
<!--begin::Body-->

<body onload="startClock()" id="kt_app_body" data-kt-app-layout="dark-sidebar" data-kt-app-header-fixed="true"
    data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true"
    data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true"
    data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
    <!--begin::Theme mode setup on page load-->

    <!--end::Theme mode setup on page load-->
    <!--begin::App-->
    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
            <!--begin::Header-->
            <%- include('./includes/header.ejs') %>
                <!--end::Header-->
                <!--begin::Wrapper-->
                <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
                    <!--begin::Sidebar-->
                    <%- include('./includes/sidebar.ejs') %>
                        <!--end::Sidebar-->
                        <!--begin::Content-->
                        <div class="content d-flex flex-column flex-column-fluid" id="kt_content">

                            <!--begin::Post-->
                            <div class="post d-flex flex-column-fluid" id="kt_post">
                                <!--begin::Container-->
                                <div id="kt_content_container" class="container-fluid">
                                    <!--begin::Form-->
                                    <form class="form mt-3" id="form-add-supplier">
                                        <!--begin::Content wrapper-->
                                        <div class="d-flex flex-column flex-column-fluid card">
                                            <!--begin::Card body-->
                                            <div class="card-body border-top pt-5">
                                                <button id="arbk-button" type="button" class="btn btn-primary">
                                                    <span class="indicator-label ">ARBK</span></button>
                                                <!--begin::Row-->
                                                <div class="row">

                                                    <!--begin::Col-->
                                                    <div class="col-md-3">
                                                        <!--begin::Input group-->
                                                        <div class="d-flex flex-column mb-2 fv-row">
                                                            <!--begin::Label-->
                                                            <label class="fs-6 fw-semibold form-label mt-3">
                                                                <span>Numri i biznesit</span>
                                                            </label>
                                                            <!--end::Label-->
                                                            <p id="arbk-supplier-error"
                                                                class="text-danger show-errors-supplier mb-0"></p>
                                                            <!--begin::Input-->
                                                            <input type="text" class="form-control form-control-solid"
                                                                name="" value="" id="arbk"
                                                                placeholder="Shëno numrin e biznesit" />
                                                        </div>
                                                        <!--end::Input group-->
                                                    </div>
                                                    <!--end::Col-->
                                                    <!--begin::Col-->
                                                    <div class="col-md-3">
                                                        <!--begin::Input group-->
                                                        <div class="d-flex flex-column mb-2 fv-row">
                                                            <!--begin::Label-->
                                                            <label class="fs-6 fw-semibold form-label mt-3">
                                                                <span>Emri i biznesit </span>
                                                            </label>
                                                            <!--end::Label-->
                                                            <p id="name-supplier-error"
                                                                class="text-danger show-errors-supplier mb-0"></p>
                                                            <!--begin::Input-->
                                                            <input type="text" class="form-control form-control-solid"
                                                                name="" value="" id="name"
                                                                placeholder="Shëno emrin e biznesit ARBK" />
                                                        </div>
                                                        <!--end::Input group-->
                                                    </div>
                                                    <!--end::Col-->
                                                    <!--begin::Col-->
                                                    <div class="col-md-3">
                                                        <!--begin::Input group-->
                                                        <div class="d-flex flex-column mb-2 fv-row">
                                                            <!--begin::Label-->
                                                            <label class="fs-6 fw-semibold form-label mt-3">
                                                                <span>Adresa e biznesit</span>
                                                            </label>
                                                            <!--end::Label-->
                                                            <p id="address-supplier-error"
                                                                class="text-danger show-errors-supplier mb-0"></p>
                                                            <!--begin::Input-->
                                                            <input type="text" class="form-control form-control-solid"
                                                                name="" value="" id="address"
                                                                placeholder="Shëno adresën e biznesit" />
                                                        </div>
                                                        <!--end::Input group-->
                                                    </div>
                                                    <!--end::Col-->
                                                    <!--begin::Col-->
                                                    <div class="col-md-3">
                                                        <!--begin::Input group-->
                                                        <div class="d-flex flex-column mb-2 fv-row">
                                                            <!--begin::Label-->
                                                            <label class="fs-6 fw-semibold form-label mt-3">
                                                                <span>Email (opsionale)</span>
                                                            </label>
                                                            <!--end::Label-->
                                                            <p id="email-supplier-error"
                                                                class="text-danger show-errors-supplier mb-0"></p>
                                                            <!--begin::Input-->
                                                            <input type="text" class="form-control form-control-solid"
                                                                name="" value="" id="email"
                                                                placeholder="Shëno email (opsionale)" />
                                                        </div>
                                                        <!--end::Input group-->
                                                    </div>
                                                    <!--end::Col-->
                                                    <!--begin::Col-->
                                                    <div class="col-md-3">
                                                        <!--begin::Input group-->
                                                        <div class="d-flex flex-column mb-2 fv-row">
                                                            <!--begin::Label-->
                                                            <label class="fs-6 fw-semibold form-label mt-3">
                                                                <span>Nr i telefonit (opsionale)</span>
                                                            </label>
                                                            <!--end::Label-->
                                                            <p id="phone_number-supplier-error"
                                                                class="text-danger show-errors-supplier mb-0"></p>
                                                            <!--begin::Input-->
                                                            <input type="text" class="form-control form-control-solid"
                                                                name="" value="" id="phone_number"
                                                                placeholder="Shëno nr e telefonit (opsionale)" />
                                                        </div>
                                                        <!--end::Input group-->
                                                    </div>
                                                    <!--end::Col-->
                                                    <!--begin::Action buttons-->
                                                    <div class="col-md-3 mt-md-11 mt-3">
                                                        <button id="" type="submit" data-kt-contacts-type="submit"
                                                            class="btn btn-primary w-100">
                                                            <span class="indicator-label">Regjistro</span>
                                                        </button>
                                                    </div>
                                                    <!--end::Action buttons-->
                                                </div>
                                                <!--end::Row-->
                                            </div>
                                        </div>

                                    </form>
                                    <!--end::Form-->
                                    <!--begin::Products-->
                                    <div class="card card-flush mt-5">
                                        <!--begin::Card header-->
                                        <div class="card-header align-items-center py-5 gap-2 gap-md-5">
                                            <!--begin::Card title-->
                                            <div class="card-title">
                                                <!--begin::Search-->
                                                <div class="d-flex align-items-center position-relative my-1">
                                                    <i class="ki-duotone ki-magnifier fs-3 position-absolute ms-4">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                    </i>
                                                    <input type="text" id="supplier-table-search"
                                                        class="form-control form-control-solid w-250px ps-12"
                                                        placeholder="Kërko..." />
                                                </div>
                                                <!--end::Search-->
                                            </div>
                                            <!--end::Card title-->
                                            <!--begin::Card toolbar-->
                                            <div class=" ms-auto">
                                                <div id="print"></div>
                                            </div>
                                            <!--end::Card toolbar-->
                                        </div>
                                        <!--end::Card header-->
                                        <!--begin::Card body-->
                                        <div class="card-body pt-0">
                                            <div class="table-responsive">
                                                <!--begin::Table-->
                                                <table class="table align-middle table-row-dashed fs-6 gy-5"
                                                    id="supplier_table">
                                                    <thead>
                                                        <tr
                                                            class="text-start text-gray-800 fw-bold fs-6 text-uppercase gs-0">
                                                            <th class="w-10px">Nr </th>
                                                            <th class="w-150px">Emri i biznesit</th>
                                                            <th class="w-150px">Adresa e biznesit</th>
                                                            <th class="w-100px">Nr i telefonit</th>
                                                            <th class="w-150px">Nr i biznesit</th>
                                                            <th class="w-150px">Email</th>
                                                            <th class="text-center w-100px">Opsionet</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="fw-semibold text-gray-700">

                                                    </tbody>
                                                </table>
                                                <!--end::Table-->
                                            </div>
                                        </div>
                                        <!--end::Card body-->
                                    </div>
                                    <!--end::Products-->
                                </div>
                                <!--end::Container-->
                            </div>
                            <!--end::Post-->
                        </div>
                        <!--end::Content-->

                        <%- include('./includes/footer.ejs') %>
                </div>
                <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->
    <!--begin::Javascript-->
    <%- include('./includes/script.ejs') %>

        <!--end::Javascript-->
        <script src="/assets/plugins/custom/datatables/datatables.bundle.js"></script>
        <!--begin::Custom Javascript(used for this page only)-->
        <script src="/assets/js/custom/apps/ecommerce/customers/listing/search/listings.js"></script>
        <script src="/assets/js/utils.js"></script>
        <script src="/assets/js/supply/supplier/supplyer_table.js"></script>
        <script src="/assets/js/supply/supplier/getArbk.js"></script>

        <!--end::Javascript-->
</body>
<!--end::Body-->

</html>