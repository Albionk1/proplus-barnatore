<!DOCTYPE html>
<!--
Author: Thana Dev 2023
Product Name: Services
-->
<html lang="en">
<!--begin::Head-->

<head>
    <!--Begin Meta links-->
    <%- include ('includes/meta-links.ejs') %>
        <!--End Meta links-->
        <link href="/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />

</head>
<!--end::Head-->
<!--begin::Body-->

<body onload="startClock()" id="kt_app_body" data-kt-app-layout="dark-sidebar" data-kt-app-header-fixed="true"
    data-kt-app-sidebar-enabled="true" data-kt-app-sidebar-fixed="true" data-kt-app-sidebar-hoverable="true"
    data-kt-app-sidebar-push-header="true" data-kt-app-sidebar-push-toolbar="true"
    data-kt-app-sidebar-push-footer="true" data-kt-app-toolbar-enabled="true" class="app-default">
    <!--begin::Theme mode setup on page load-->

    <!--end::Theme mode setup on page load-->
    <!--begin::App-->
    <div class="d-flex flex-column flex-root app-root" id="kt_app_root">
        <!--begin::Page-->
        <div class="app-page flex-column flex-column-fluid" id="kt_app_page">
            <!--begin::Header-->
            <%- include('./includes/header.ejs') %>
                <!--end::Header-->
                <!--begin::Wrapper-->
                <div class="app-wrapper flex-column flex-row-fluid" id="kt_app_wrapper">
                    <!--begin::Sidebar-->
                    <%- include('./includes/sidebar.ejs') %>
                        <!--end::Sidebar-->
                        <!--begin::Content-->
                        <div class="content d-flex flex-column flex-column-fluid" id="kt_content">

                            <!--begin::Post-->
                            <div class="post d-flex flex-column-fluid" id="kt_post">
                                <!--begin::Container-->
                                <div id="kt_content_container" class="container-fluid">
                                    <div class="row mt-3">
                                        <div class="col-lg-7">
                                            <!--begin::Form-->
                                            <form class="form mt-5" method="" action="">
                                                <!--begin::Content wrapper-->
                                                <div class="d-flex flex-column flex-column-fluid card">
                                                    <!--begin::Card body-->
                                                    <div class="card-body border-top pt-5">
                                                        <!--begin::Row-->
                                                        <div class="row">
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Emri dhe mbiemri</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                        disabled value="<%=useri.full_name  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Fjalëkalimi</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="password" class="form-control" name=""
                                                                        disabled value="12345678" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
    
    <% const role = {superadmin:'Admin',managment:'Menagjment',pos:'Pos'} %>
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Pozicioni në kompani</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <input type="text" class="form-control" name=""
                                                                    disabled value="<%=role[useri.role]  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Privilegjet në kompani</span>
                                                                    </label>
                                                                    <%  const access={'sales_many':'Shitje me shumic','statistic':'Statistikat','cash_register':'Arka','stock':'Stoku','actions':'Aksionet','offerts':'Oferto','supply':'Furnizimet','intern_exhange':'Levizje interne','article':'Artikujt','clients':'Klientët','workers':'Puntorët','company':'Kompania','partners':'Partnerët','pos':'Pos','arc':'Arka','admin':'Admin'} %>
                                                                    <!--end::Label-->
                                                                    <% const userAccessArray = useri.access.split(',')
                                                                    const accessLabels = userAccessArray.map(accessRight => access[accessRight]); %>
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                    disabled value="<%= accessLabels.join(', ')  %>" />
                                                                    <!--end::Input-->
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Njësia</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                    disabled value="<%= useri.unit?useri.unit.unit_name:''  %>" />
                                                                    <!--end::Input-->
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Numri personal(opsional)</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                        disabled value="<%=useri.phone_number  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Llogari bankare</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                        disabled value="<%=useri.bank  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Paga neto</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                        disabled value="<%=useri.salary_neto  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="d-flex flex-column mb-7 fv-row">
                                                                    <!--begin::Label-->
                                                                    <label class="fs-6 fw-semibold form-label mt-3">
                                                                        <span>Paga bruto</span>
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <!--begin::Input-->
                                                                    <input type="text" class="form-control" name=""
                                                                        disabled value="<%=useri.salary_bruto  %>" />
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                            <!--begin::Col-->
                                                            <div class="col-xl-4 col-lg-6 col-md-4 col-sm-6">
                                                                <!--begin::Input group-->
                                                                <div class="fv-row mb-7">
                                                                    <!--begin::Label-->
                                                                    <label
                                                                        class="col-lg-3 col-form-label fw-semibold fs-6 ">Koment
                                                                    </label>
                                                                    <!--end::Label-->
                                                                    <textarea class="form-control" name="" disabled
                                                                        rows="2"><%=useri.comment  %></textarea>
                                                                    <!--end::Input group-->
                                                                </div>
                                                                <!--end::Input group-->
                                                            </div>
                                                            <!--end::Col-->
                                                        </div>
                                                        <!--end::Row-->
                                                    </div>
                                                </div>

                                            </form>
                                        </div>
                                        <!--end::Form-->
                                        <div class="col-lg-5">
                                            <div class="card">
                                                <div class="card-body">
                                                    <div class="row mt-3 mt-lg-0">
                                                        <div class="col-xl-4 col-lg-6 col-md-3 col-sm-6 col-12">
                                                            <!--begin::Statistics Widget 5-->
                                                            <a class="card border-primary bg-primary mb-5 mb-xl-2">
                                                                <!--begin::Body-->
                                                                <div
                                                                    class="card-body card-body d-flex flex-column align-items-center justify-content-center p-5">

                                                                    <i class="fas fa-chart-bar text-white fs-2x"></i>
                                                                    <div class="text-white fw-bold fs-2 text-center">
                                                                        Max shitje: <br>
                                                                        <span><%=parseFloat(max).toFixed(2)%> €</span>
                                                                    </div>
                                                                </div>
                                                                <!--end::Body-->
                                                            </a>
                                                            <!--end::Statistics Widget 5-->
                                                        </div>
                                                        <div class="col-xl-4 col-lg-6 col-md-3 col-sm-6 col-12">
                                                            <!--begin::Statistics Widget 5-->
                                                            <a class="card border-success bg-success mb-5 mb-xl-2">
                                                                <!--begin::Body-->
                                                                <div
                                                                    class="card-body card-body d-flex flex-column align-items-center justify-content-center p-5">

                                                                    <i class="fas fa-chart-line text-white fs-2x"></i>
                                                                    <div class="text-white fw-bold fs-2 text-center">
                                                                        Min shitje: <br>
                                                                        <span><%=parseFloat(min).toFixed(2)%> €</span>
                                                                    </div>
                                                                </div>
                                                                <!--end::Body-->
                                                            </a>
                                                            <!--end::Statistics Widget 5-->
                                                        </div>
                                                        <div class="col-xl-4 col-lg-6 col-md-3 col-sm-6 col-12">
                                                            <!--begin::Statistics Widget 5-->
                                                            <a class="card border-danger bg-danger mb-5 mb-xl-2">
                                                                <!--begin::Body-->
                                                                <div
                                                                    class="card-body card-body d-flex flex-column align-items-center justify-content-center p-5">

                                                                    <i class="fas fa-credit-card text-white fs-2x"></i>
                                                                    <div class="text-white fw-bold fs-2 text-center">
                                                                        Nr i kartave: <br>
                                                                        <span><%=loyaltyCard  %></span>
                                                                    </div>
                                                                </div>
                                                                <!--end::Body-->
                                                            </a>
                                                            <!--end::Statistics Widget 5-->
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--begin::Products-->
                                    <div class="card card-flush d-none">
                                        <!--begin::Card header-->
                                        <div class="card-header align-items-center py-5 gap-2 gap-md-5">
                                            <!--begin::Card title-->
                                            <div class="card-title">
                                                <!--begin::Search-->
                                                <div class="d-flex align-items-center position-relative my-1">
                                                    <i class="ki-duotone ki-magnifier fs-3 position-absolute ms-4">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                    </i>
                                                    <input type="text" data-kt-ecommerce-order-filter="search"
                                                        class="form-control form-control-solid w-250px ps-12"
                                                        placeholder="Kërko..." />
                                                </div>
                                                <!--end::Search-->
                                            </div>
                                            <!--end::Card title-->
                                            <!--begin::Card toolbar-->
                                            <div id="print" class="ms-auto"></div>
                                            <!--end::Card toolbar-->
                                        </div>
                                        <!--end::Card header-->
                                        <!--begin::Card body-->
                                        <div class="card-body pt-0">
                                            <div class="table-responsive">
                                                <!--begin::Table-->
                                                <table class="table align-middle table-row-dashed fs-6 gy-5"
                                                    id="kt_ecommerce_sales_table">
                                                    <thead>
                                                        <tr
                                                            class="text-start text-gray-800 fw-bold fs-6 text-uppercase gs-0">
                                                            <th class="w-10px">Nr </th>
                                                            <th class="w-150px">Emri mbiemri</th>
                                                            <th class="w-150px">Pozicioni</th>
                                                            <th class="w-150px">Autorizimet</th>
                                                            <th class="w-150px">Paga neto</th>
                                                            <th class="w-150px">Paga bruto</th>
                                                            <th class="w-150px">Data e pagesës</th>
                                                            <th class="w-150px">Muaji</th>
                                                            <th class="w-150px">Viti / Statusi</th>
                                                            <th class="text-end w-100px">Opsionet</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="fw-semibold text-gray-700">
                                                        <tr>
                                                            <td data-kt-ecommerce-order-filter="order_id"
                                                                class="text-gray-700 fw-bold text-center">1</td>
                                                            <td class="text-gray-700 fw-bold">Filan Fisteku</td>
                                                            <td class="text-gray-700 fw-bold">Admin</td>
                                                            <td class="text-gray-700 fw-bold">Admin</td>
                                                            <td class="text-gray-700 fw-bold">450 €</td>
                                                            <td class="text-gray-700 fw-bold">500 €</td>
                                                            <td class="text-gray-700 fw-bold">22.05.2023</td>
                                                            <td class="text-gray-700 fw-bold">Shtator</td>
                                                            <td class="text-gray-700 fw-bold">2024 / Aktiv</td>
                                                            <td class="text-center fw-bold pt-3">
                                                                <a style="cursor: pointer;">
                                                                    <i class="fas fa-edit text-primary fs-5"></i>
                                                                </a>
                                                                <a href="" class="ms-10" data-bs-toggle="tooltip"
                                                                    aria-label="Fshij" data-bs-original-title="Fshij">
                                                                    <i class="fas fa-trash-alt text-danger fs-5"></i>
                                                                </a>
                                                            </td>

                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <!--end::Table-->
                                            </div>
                                        </div>
                                        <!--end::Card body-->
                                    </div>
                                    <!--end::Products-->
                                </div>
                                <!--end::Container-->
                            </div>
                            <!--end::Post-->
                        </div>
                        <!--end::Content-->

                        <%- include('./includes/footer.ejs') %>
                </div>
                <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
    <!--end::App-->
    <!--begin::Javascript-->
    <%- include('./includes/script.ejs') %>

        <!--end::Javascript-->
        <script src="/assets/plugins/custom/datatables/datatables.bundle.js"></script>
        <!--begin::Custom Javascript(used for this page only)-->
        <script src="/assets/js/custom/apps/ecommerce/customers/listing/search/listing.js"></script>

        <!--end::Javascript-->
</body>
<!--end::Body-->

</html>