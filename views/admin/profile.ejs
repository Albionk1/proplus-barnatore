                                                            <div class="col-lg-8 fv-row">
                                                                <input type="tel" name=""
                                                                    class="form-control form-control-lg form-control-solid"
                                                                    placeholder="Numri i telefonit" />
                                                            </div>
                                                            <!--end::Col-->
                                                        </div>
                                                        <!--end::Input group-->
                                                        <!--begin::Input group-->
                                                        <div class="row mb-6">
