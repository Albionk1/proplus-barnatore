                                                                    placeholder="Qyteti" />
                                                            </div>
                                                            <!--end::Col-->
                                                        </div>
                                                        <!--end::Input group-->
                                                        <!--begin::Input group-->
                                                        <div class="row mb-6">
                                                            <!--begin::Label-->
                                                            <label
                                                                class="col-lg-4 col-form-label fw-semibold fs-6">Adresa
                                                                elektronike
                                                            </label>
                                                            <!--end::Label-->
                                                            <!--begin::Col-->
                                                            <div class="col-lg-8 fv-row">
                                                                <input type="email" name=""
                                                                    class="form-control form-control-lg form-control-solid"
                                                                    placeholder="Adresa elektronike" />
