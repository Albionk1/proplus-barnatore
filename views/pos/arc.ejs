    <script>
        // Function to calculate and update the total for a specific denomination
        function calculateTotal(denomination) {
            const inputElement = document.querySelector(`input[name="${denomination}"]`);
            const totalElement = document.querySelector(`#total_${denomination.replace(".", "\\.")}`);
            let value = parseFloat(inputElement.value) || 0;

            // Ensure the value is not negative
            value = Math.max(value, 0);

            // Update the input field with the non-negative value
            inputElement.value = value;

            const denominationTotal = value * parseFloat(denomination);
            totalElement.textContent = denominationTotal.toFixed(2) + " €";
            updateFinalTotal();
        }

        // Function to update the final total by summing all denomination totals
        function updateFinalTotal() {
            const denominations = ["0.05", "0.10", "0.20", "0.50", "1", "2", "5", "10", "20", "50", "100", "200", "500"];
            let finalTotal = 0;
            for (const denomination of denominations) {
                const inputElement = document.querySelector(`input[name="${denomination}"]`);
                const value = parseFloat(inputElement.value) || 0;
                finalTotal += value * parseFloat(denomination);
            }
            document.querySelector("#final_total").textContent = finalTotal.toFixed(2) + " €";
        }

        // Function to clear all input fields and total values
        function clearInputs() {
            const inputElements = document.querySelectorAll('input[type="number"]');
            for (const inputElement of inputElements) {
                inputElement.value = 0;
            }

            // Reset total values to 0
            const denominations = ["0.05", "0.10", "0.20", "0.50", "1", "2", "5", "10", "20", "50", "100", "200", "500"];
            for (const denomination of denominations) {
                const totalElement = document.querySelector(`#total_${denomination.replace(".", "\\.")}`);
                totalElement.textContent = "0 €";
            }

            // Update the final total to 0
            document.querySelector("#final_total").textContent = "0 €";
        }

        // Attach event listeners to input fields to calculate totals dynamically
        const denominations = ["0.05", "0.10", "0.20", "0.50", "1", "2", "5", "10", "20", "50", "100", "200", "500"];
        for (const denomination of denominations) {
            const inputElement = document.querySelector(`input[name="${denomination}"]`);
            inputElement.addEventListener("input", () => calculateTotal(denomination));
        }

        // Initialize the final total when the page loads
        updateFinalTotal();
    </script>





